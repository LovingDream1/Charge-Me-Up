/* 设置swiper组件的宽高 */
.swiper{
  width: 100%;
  height: 600rpx;
}
/* 设置swiper组件里面图片的宽高 */
.image{
  width: 100%;
  height: 600rpx;
}
 
.container{
  height: 100%;
  display: flex;
  flex-direction: column;
  padding-top: 100rpx;
}
 
.title{
  display:flex;
  font-size: larger;
}
 
.inputRow{
  margin-top: 100rpx;
  display: flex;
  flex-direction: row;
  padding-bottom: 20rpx;
  border-bottom: .5px solid rgba(0, 0, 0, 0.1);
}
 
/*分割线样式*/
.divLine{
  background: #E0E3DA;
  width: 80%;
  height: 5rpx;
  margin: 10rpx 150rpx;
}
 
